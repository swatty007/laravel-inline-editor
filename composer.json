{
    "name": "swatty007/laravel-inline-editor",
    "description": "Laravel package, which allows inline editing of model specific content blocks.",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Niehoff",
            "email": "info@martin-niehoff.de"
        }
    ],
    "require": {
        "php": ">=5.6",
        "illuminate/support": "5.*|6.*|7.*|8.*",
        "illuminate/database": "^5.2"
    },
    "extra": {
        "laravel": {
            "providers": [
                "swatty007\\LaravelInlineEditor\\InlineEditorServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "swatty007\\LaravelInlineEditor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "swatty007\\LaravelInlineEditor\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4"
    }
}
